EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$CONFIGURATION_BUILD_DIR/SnapKit" "$CONFIGURATION_BUILD_DIR/YYKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/SnapKit/SnapKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/YYKit/YYKit.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "SnapKit" -framework "YYKit"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_ROOT = ${SRCROOT}/Pods
